{
  "name": "bootstrapserver",
  "version": "0.1.0",
  "author": "Mario Blanco",
  "private": true,
  "proxy": "https://localhost:5000",
  "main": "index.js",
  "license": "ISC",
  "dependencies": {
    "express": "4.17.3",
    "ts-node-dev": "1.1.8"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.6.1",
    "ts-node": "10.7.0",
    "tslint": "6.1.3",
    "typescript": "4.6.3"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "rm -rfv node_modules && rm -rfv build",
    "init": "npm install",
    "lint": "eslint './**/*.{ts,tsx,json}'",
    "lint:fix": "eslint --fix './**/*.{ts,tsx,json}'",
    "pretty": "prettier --check  \"./**/*.{js,ts,tsx}\"",
    "pretty:write": "prettier --write \"./**/*.{js,json,ts,tsx}\"",
    "start:server": "tsnd --respawn server.ts && wait-on tcp:5000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
